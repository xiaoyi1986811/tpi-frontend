yle":1,"linewidth":3,"page":3,"rect":[410.6249877624217,507.74998486787126,410.6249877624217,589.4999824315315,391.124988343567,253.87499243393563,392.6249882988635,596.9999822080141,377.62498874589835,247.49999262392544,380.9999886453155,367.874989036471],"type":1,"uuid":"689ef947a9a69b120faa065a195ed8aa"},{"date":"2025-03-24 21:16:44","docid":0,"id":34,"imgfile":"1b2531f86a4e1ae362732bdc523f212b.png","linecolor":"ffaa0000","linestyle":1,"linewidth":3,"page":3,"rect":[330.74999014288215,448.1249866448347,329.9999901652339,582.3749826438731,315.749990589917,246.37499265745305,316.8749905563894,489.37498541548894],"type":1,"uuid":"c0c9b4a4fb163511a4b8133cd07c316b"},{"date":"2025-03-24 21:22:08","docid":0,"id":36,"imgfile":"bb765b5db8d6eec2bd5b4f8e7e07efe8.png","linecolor":"ffaa0000","linestyle":1,"linewidth":3,"page":3,"rect":[278.249991707504,249.7499925568702,280.12499165162467,592.1249823533004,264.7499921098354,247.1249926351013,264.37499212101125,314.2499906346205],"type":1,"uuid":"363663d73ad31c3b01632af08531a9bf"}]}